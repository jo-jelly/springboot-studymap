 {{>layout/header}}
<body>

<div id="main_body">
     <div class="col-md-12">
         <div class="row">
             <div class="col-md-6">
                 <a href="/project/save" role="button" class="btn btn-primary">프로젝트 글 등록</a>
                 <a href="/studyGroup/save" role="button" class="btn btn-primary">스터디 그룹 글 등록</a>
                 {{#userName}} <!--유저명이 들어왔을 경우에 사용 mustache는 if()문을 사용할수 없다.-->
                     Logged in as: <span id="user">{{userName}}</span>
                     <a href="/logout" class="btn btn-info active" role="button">Logout</a>
                 {{/userName}}
                 {{^userName}} <!-- 유저명이 없을경우-->
                     <a href="/oauth2/authorization/google" class="btn btn-success active" role="button">Google Login</a>
                     <a href="/oauth2/authorization/naver" class="btn btn-success active" role="button">Naver Login</a>
                 {{/userName}}
             </div>
         </div>
     </div>
         <br>

 <div>
     <div id="main_aside">
     <aside>
         <h1>포럼게시판</h1>
         {{#forum}}
             <div class="card border-secondary mb-12" id="card_list" style="max-width: 55rem;"> <!-- 회색 테두리-->
                 <div class="card-header" id="card-header">
                     <span  class="btn btn-primary btn-sm">{{area}}</span>
                     <span  class="badge bg-warning">{{state}}</span>

                     <a href="/forum/update/{{id}}">{{author}}</a>
                 </div>
                 <div class="card-body">
                     <h4 class="card-title">
                         <a href="/forum/view/{{id}}">{{title}}</a>
                     </h4>
                     <p role="img" aria-label="eye" class="anticon anticon-eye">
                         <svg viewBox="64 64 896 896" focusable="false" data-icon="eye" width="1em" height="1em" fill="currentColor" aria-hidden="true">
                             <path d="M942.2 486.2C847.4 286.5 704.1 186 512 186c-192.2 0-335.4 100.5-430.2 300.3a60.3 60.3 0 000 51.5C176.6 737.5 319.9 838 512 838c192.2 0 335.4-100.5 430.2-300.3 7.7-16.2 7.7-35 0-51.5zM512 766c-161.3 0-279.4-81.8-362.7-254C232.6 339.8 350.7 258 512 258c161.3 0 279.4 81.8 362.7 254C791.5 684.2 673.4 766 512 766zm-4-430c-97.2 0-176 78.8-176 176s78.8 176 176 176 176-78.8 176-176-78.8-176-176-176zm0 288c-61.9 0-112-50.1-112-112s50.1-112 112-112 112 50.1 112 112-50.1 112-112 112z">
                             </path>
                         </svg>
                         {{views}}</p>
                 </div>
             </div>
         {{/forum}}
     </aside>
     </div>

     <div id="main_row">
         <h1>프로젝트 모집</h1>
         {{#project}}
             <div class="card border-primary mb-12" id="card_list" style="max-width: 55rem;"> <!-- 파란색 테두리-->
                 <div class="card-header" id="card-header">
                     <a href="/studyGroup/update/{{id}}">{{author}} &nbsp;&nbsp;{{modifiedDate}} </a>
                 </div>
                 <div class="card-body">
                     <h4 class="card-title">
                         <a href="/studyGroup/view/{{id}}">{{title}}</a>
                     </h4>
                     <p role="img" aria-label="eye" class="anticon anticon-eye">
                         <svg viewBox="64 64 896 896" focusable="false" data-icon="eye" width="1em" height="1em" fill="currentColor" aria-hidden="true">
                             <path d="M942.2 486.2C847.4 286.5 704.1 186 512 186c-192.2 0-335.4 100.5-430.2 300.3a60.3 60.3 0 000 51.5C176.6 737.5 319.9 838 512 838c192.2 0 335.4-100.5 430.2-300.3 7.7-16.2 7.7-35 0-51.5zM512 766c-161.3 0-279.4-81.8-362.7-254C232.6 339.8 350.7 258 512 258c161.3 0 279.4 81.8 362.7 254C791.5 684.2 673.4 766 512 766zm-4-430c-97.2 0-176 78.8-176 176s78.8 176 176 176 176-78.8 176-176-78.8-176-176-176zm0 288c-61.9 0-112-50.1-112-112s50.1-112 112-112 112 50.1 112 112-50.1 112-112 112z">
                             </path>
                         </svg>
                         {{views}}</p>
                 </div>
             </div>
         {{/project}}

         <br>
    <!--스터디 그룹 목록 출력 영역-->
         <h1>스터디그룹 모집</h1>
         {{#studyGroup}}
             <div class="card border-primary mb-12" id="card_list" style="max-width: 55rem;"> <!-- 파란색 테두리-->
                 <div class="card-header" id="card-header">
                     <span  class="btn btn-primary btn-sm">{{area}}</span>
                     <span  class="badge bg-warning">{{state}}</span>

                     <a href="/studyGroup/update/{{id}}">{{author}} &nbsp;&nbsp;{{modifiedDate}} </a>
                 </div>
                 <div class="card-body">
                     <h4 class="card-title">
                         <a href="/studyGroup/view/{{id}}">{{title}}</a>
                     </h4>
                     <p role="img" aria-label="eye" class="anticon anticon-eye">
                         <svg viewBox="64 64 896 896" focusable="false" data-icon="eye" width="1em" height="1em" fill="currentColor" aria-hidden="true">
                             <path d="M942.2 486.2C847.4 286.5 704.1 186 512 186c-192.2 0-335.4 100.5-430.2 300.3a60.3 60.3 0 000 51.5C176.6 737.5 319.9 838 512 838c192.2 0 335.4-100.5 430.2-300.3 7.7-16.2 7.7-35 0-51.5zM512 766c-161.3 0-279.4-81.8-362.7-254C232.6 339.8 350.7 258 512 258c161.3 0 279.4 81.8 362.7 254C791.5 684.2 673.4 766 512 766zm-4-430c-97.2 0-176 78.8-176 176s78.8 176 176 176 176-78.8 176-176-78.8-176-176-176zm0 288c-61.9 0-112-50.1-112-112s50.1-112 112-112 112 50.1 112 112-50.1 112-112 112z">
                             </path>
                         </svg>
                         {{views}}</p>
                 </div>
             </div>
         {{/studyGroup}}
 </div>
 </div>
</div>
</body>
    {{>layout/footer}}